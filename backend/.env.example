# Server configuration
PORT=3001

# Blockchain network (sepolia, localhost)
NETWORK=sepolia
RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key-here

# Admin key for protected endpoints (create a random string for this)
ADMIN_KEY=your_admin_key_here

# Private key for the account that will submit transactions
# WARNING: Never commit the actual .env file with this key to git
PRIVATE_KEY=your_private_key_here

# Scheduler interval in milliseconds (default: 5 minutes)
SCHEDULER_INTERVAL=300000 